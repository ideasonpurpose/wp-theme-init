{
  "name": "ideasonpurpose/wp-theme-init",
  "description": "A common baseline of repeated functions, filters and actions used across WordPress themes.",
  "license": "MIT",
  "keywords": [
    "wordpress",
    "plugin",
    "init",
    "Ideas On Purpose"
  ],
  "authors": [
    {
      "name": "Ideas On Purpose",
      "homepage": "https://www.ideasonpurpose.com"
    },
    {
      "name": "Joe Maller",
      "email": "joe@ideasonpurpose.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "IdeasOnPurpose\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "require-dev": {
    "ideasonpurpose/wp-test-stubs": "dev-main@dev",
    "php-stubs/acf-pro-stubs": "dev-master",
    "php-stubs/wordpress-stubs": "dev-master"
  },
  "archive": {
    "exclude": [
      "/.git*",
      "/tests",
      "CHANGELOG.md",
      "docker-compose.yml",
      "package*",
      "phpunit.xml"
    ]
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/joemaller/acf-pro-stubs",
      "no-api": true
    }
  ]

}
